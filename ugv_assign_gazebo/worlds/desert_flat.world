<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="desert_world">
    <include> <!-- Add in a light source -->
      <uri>model://sun</uri>
    </include>
    <include> <!-- Add your custom model -->
      <uri>model://desert_flat</uri>
      <name>desert_flat</name>
      <pose>-120 0 0 0 0 0</pose>
    </include>
    <scene>
      <sky>
        <clouds>
          <speed>12</speed>
        </clouds>
      </sky>
    </scene>
  </world>
</sdf>